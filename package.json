{
  "name": "weather",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p sass start-react",
    "build": "npm-run-all build-css && react-scripts build",
    "sass": "node-sass --watch src/styles/scss -o  src/styles/css",
    "build-css": "node-sass src/styles/scss -o  src/styles/css  --style compressed"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  }
}
